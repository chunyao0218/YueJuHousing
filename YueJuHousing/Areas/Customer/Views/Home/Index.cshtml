    <!-- 最新上架物件 -->
    <section id="AboutArea" class="wn__newsletter__area newsletter--bg">
        <div class="main-title text-center" style="margin-top:50px;">
            <h3 class="section-head mt55">
                最新上架物件
                <p>嚴選最新的物件</p>
            </h3>
        </div>
        <div class="hotbuild-1 owl-carousel owl-theme hotbuild">
            @foreach (var land in Model)
            {
                <div class="item">
                    <div class="scroll-wrapper clearfix triggerOne_show" id="hotBuild_@land.Id" data-id="hotBuild">
                        <div class="clearfix">
                            <div class="scroll-main hot-builds">
                                <div class="scroll-list newbuild-hot clearfix" style="left: 0px;">
                                    <dl class="selected">
                                        <a asp-controller="AllLand" asp-action="Details" asp-route-landId="@land.Id" target="_blank">
                                        <dt>
                                            <span class="shoucang">
                                                    @*<strong>@land.ImageCount張照片</strong>*@
                                                <object>
                                                    <a data-text="@land.Id" data-colle="0" data-bind="8" google-data-stat="新版首頁_點擊收藏_新建案廣告" href="javascript:;" title="點擊收藏"></a>
                                                </object>
                                            </span>
                                            <img src="@land.ImageUrl" alt="@land.AdTitle" width="270" height="180">
                                            <div class="newbuild-title">
                                                <div class="tel-shade"></div>
                                                <div class="newbuild-tel"><i b-pysnvdqbir="" class="fa fa-phone" aria-hidden="true"></i> @land.CellPhoneNumber</div>
                                            </div>
                                        </dt>
                                        <dd>
                                            <div class="newbuild-info clearfix">
                                                <div class="newbuild-name">
                                                    <strong>@land.AdTitle</strong> @*[@land.LandSerialNumber]*@
                                                </div>
                                                <div class="newbuild-price">
                                                    <strong>@land.Price 萬@*<small>@land.SquareMetersUnit</small>*@</strong>
                                                </div>
                                            </div>
                                            <p>
                                                    @*<i class="fa fa-quote-left mr5"></i>*@ @land.FeatureDescribe @*<i class="fa fa-quote-right quote-right"></i>*@
                                                <span class="newbuild-label ml10">查看更多<em></em></span>
                                            </p>
                                        </dd>
                                        </a>
                                    </dl>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
